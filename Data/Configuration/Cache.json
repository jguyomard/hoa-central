{
    "frontend": {
        "lifetime":          3600,
        "serialize_content": true,
        "make_id_with": {
            "get":           true,
            "post":          true,
            "cookie":        true,
            "session":       true,
            "files":         true
        }
    },
    "backend": {
        "cache_directory":   "/tmp/",
        "compress": {
            "active":        true,
            "level":         true
        },
        "database": {
            "host":          "127.0.0.0",
            "port":          11211,
            "persistent":    true
        }
    }
}
